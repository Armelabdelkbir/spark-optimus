# Environment Variables Template
# Copy this file to .env and fill in your values

# Google AI API Key for Gemini (required for AI-powered analysis)
GEMINI_API_KEY=your_api_key_here

# Spark History Server URL (optional, defaults to mock data)
SPARK_HISTORY_SERVER_URL=http://localhost:18080

# Use mock data for demo (true/false)
USE_MOCK_DATA=true
